@model EmergencyManagementSystem.Service.Models.MemberModel

@{
    ViewData["Title"] = "Teste";
    Layout = "~/Views/Shared/_Layout.cshtml";

}

@Html.AntiForgeryToken()
<div class="content-header sty-one">
    <h1 class="text-white">Composição de Equipe </h1>
</div>

<div class="content">
    <div class="info-box">
        <div class="row">
            <div class="col-lg-6 m-b-3">
                <h4 class="text-white">Profissionais</h4>
                <div class="col-lg-12 form-group-sm row ">
                    <input id="demo-input-search2" name="" placeholder="Ocupação" class="form-control col-md-3" type="text" value="@ViewBag.VehicleName">
                    <input type="submit" class="btn btn-secondary btn-nor" value="Pesquisar" />
                </div>

                <div class="table-responsive">
                    <table class="table">
                        <thead class="bg-primary text-white">
                            <tr>
                                <th scope="col">Id</th>
                                <th scope="col">Nome</th>
                                <th scope="col">Ocupação</th>
                                <th scope="col"></th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <th scope="row">1</th>
                                <td>Mark</td>
                                <td>Otto</td>
                                <td class="jsgrid-cell jsgrid-control-field jsgrid-align-center" style="width: 50px;">
                                    <span class="pull-right">
                                        <div class="col-md-3">
                                            <a class="btn btn-outline-success btn-sm" title="Add usuário"><i class="fa fa-user-md"></i></a>
                                        </div>
                                    </span>
                                </td>
                            </tr>
                            <tr>
                                <th scope="row">2</th>
                                <td>Jacob</td>
                                <td>Thornton</td>
                                <td class="jsgrid-cell jsgrid-control-field jsgrid-align-center" style="width: 50px;">
                                    <span class="pull-right">
                                        <div class="col-md-3">
                                            <a class="btn btn-outline-success btn-sm" title="Add usuário"><i class="fa fa-user-md"></i></a>
                                        </div>
                                    </span>
                                </td>
                            </tr>
                            <tr>
                                <th scope="row">3</th>
                                <td>Larry</td>
                                <td>the Bird</td>
                                <td class="jsgrid-cell jsgrid-control-field jsgrid-align-center" style="width: 50px;">
                                    <span class="pull-right">
                                        <div class="col-md-3">
                                            <a class="btn btn-outline-success btn-sm" title="Add usuário"><i class="fa fa-user-md"></i></a>
                                        </div>
                                    </span>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>

            <div class="col-lg-6 m-b-3">
                <h4 class="text-white">Veículo</h4>

                <div class="form-group has-feedback">
                    <select class="form-control" name="Veículos" id="Veículos">
                    </select>
                    <span class="fa fa-user form-control-feedback" aria-hidden="true"></span>
                </div>
                <h4 class="text-white">Equipe atual</h4>
                <div class="table-responsive">
                    <table class="table">
                        <thead class="bg-primary text-white">
                            <tr>
                                <th scope="col">Id</th>
                                <th scope="col">Funcionário</th>
                                <th scope="col">Ocupação</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <th scope="row">1</th>
                                <td>Mark</td>
                                <td>Otto</td>
                            </tr>
                            <tr>
                                <th scope="row">2</th>
                                <td>Jacob</td>
                                <td>Thornton</td>
                            </tr>
                            <tr>
                                <th scope="row">3</th>
                                <td>Larry</td>
                                <td>the Bird</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>


        </div>
    </div>
</div>