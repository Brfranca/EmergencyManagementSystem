<script>
    //Cuidado ao mexer

    $(function () {
        $('#example1').DataTable()
        $('#example2').DataTable({
            'paging': false,
            'lengthChange': false,
            'searching': false,
            'ordering': false,
            'info': false,
            'autoWidth': false,
            'scrollY': "200px",
            'scrollCollapse': true,
            'responsive': true
        })
    })

    function ConcatFields(idPatient) {

        var pa = $('#PA-' + idPatient);
        var fc = $('#FC-' + idPatient);
        var fr = $('#FR-' + idPatient);
        var temp = $('#Temp-' + idPatient);
        var sat02 = $('#Sat02-' + idPatient);
        var hgt = $('#HGT-' + idPatient);
        var glas = $('#Glasgow-' + idPatient);

        var evalution = $('#Evaluation-' + idPatient).val();

        var result = '';
        if (pa.val().trim() != '')
            result += 'PA: ' + pa.val() + ' - ';
        if (fc.val().trim() != '')
            result += 'FC ' + fc.val() + ' - ';
        if (fr.val().trim() != '')
            result += 'FR ' + fr.val() + ' - ';
        if (temp.val().trim() != '')
            result += 'Temp ' + temp.val() + ' - ';
        if (sat02.val().trim() != '')
            result += 'Sat02 ' + sat02.val() + ' - ';
        if (hgt.val().trim() != '')
            result += 'HGT ' + hgt.val() + ' - ';
        if (glas.val().trim() != '')
            result += 'Glasgow ' + glas.val() + ' ';

        if (evalution.trim() != '') {
            $('#Evaluation-' + idPatient).val(evalution + ' ' + result);
        } else {
            $('#Evaluation-' + idPatient).val(result);
        }

        pa.val('');
        fc.val('');
        fr.val('');
        temp.val('');
        sat02.val('');
        hgt.val('');
        glas.val('');

    }
    var qtdPatient = 0;

    RefreshQtdPatient();

    function RefreshQtdPatient() {
        if (@Model.Patients.Count() > 0) {
            qtdPatient = @Model.Patients.Count();
        }
