            .replace('FR-', 'FR-' + numberPacient)
            .replace('Temp-', 'Temp-' + numberPacient)
            .replace('Sat02-', 'Sat02-' + numberPacient)
            .replace('HGT-', 'HGT-' + numberPacient)
            .replace('Glasgow-', 'Glasgow-' + numberPacient)
